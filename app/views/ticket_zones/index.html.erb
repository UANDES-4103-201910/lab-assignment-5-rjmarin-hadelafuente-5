<p id="notice"><%= notice %></p>

<h1>Ticket Zones</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_zones.each do |ticket_zone| %>
      <tr>
        <td><%= link_to 'Show', ticket_zone %></td>
        <td><%= link_to 'Edit', edit_ticket_zone_path(ticket_zone) %></td>
        <td><%= link_to 'Destroy', ticket_zone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket Zone', new_ticket_zone_path %>
